{
  "name": "quotator",
  "version": "0.1.0",
  "description": "Quote generator",
  "main": "index.js",
  "author": "Brian Dellaterra",
  "license": "UNLICENSED",
  "ava": {
    "files": [
      "test/*test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "scripts": {
    "cover": "cross-env NODE_PATH=src/js JS_TEST_LIB=ava nyc ava",
    "tap": "cross-env NODE_PATH=src/js JS_TEST_LIB=ava ava --tap",
    "test": "cross-env NODE_PATH=src/js JS_TEST_LIB=ava ava",
    "watch": "cross-env NODE_PATH=src/js JS_TEST_LIB=ava ava --watch",
    "lint": "eslint . --quiet --cache --ignore-path .gitignore",
    "mon": "nodemon --watch package.json --watch webpack.config.babel.js --watch conf/webpack --exec npm run dev",
    "dev": "webpack -d && webpack-dev-server --watch-poll",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint_d": "^4.2.5",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "redux-devtools": "^3.4.0",
    "require-dir": "^0.3.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "cross-env": "^4.0.0",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-act": "^1.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.3.1"
  }
}
